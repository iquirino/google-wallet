{
  "name": "google-wallet",
  "version": "1.0.16",
  "description": "Google wallet library for Node.js",
	"repository": "iquirino/google-wallet",
  "keywords": ["google-wallet", "loyalty"],
  "author": "Igor Quirino",
  "license": "ISC",
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    },
    "./lib/esm/*": {
      "types": "./lib/esm/*.d.ts",
      "import": "./lib/esm/*.js",
      "require": "./lib/cjs/*.js",
      "default": "./lib/esm/*.js"
    },
    "./lib/cjs/*": {
      "types": "./lib/cjs/*.d.ts",
      "import": "./lib/esm/*.js",
      "require": "./lib/cjs/*.js",
      "default": "./lib/esm/*.js"
    }
  },
  "scripts": {
    "generate": "ts-node --esm scripts/generate.ts",
    "build": "make",
    "prepack": "make"
  },
  "dependencies": {
    "google-auth-library": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "cheerio": "1.0.0-rc.12",
    "got": "^13.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
